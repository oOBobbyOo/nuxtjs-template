{
  "name": "vue-vite-admin",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:test": "vite build --mode test",
    "build:gzip": "vite build",
    "build:report": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:prettier": "prettier --config .prettierrc --write \"./src/**/*.{ts,tsx,vue}\"",
    "lint:style": "stylelint --fix \"./src/**/*.{css,less,vue}\"",
    "typecheck": "vue-tsc --noEmit",
    "postinstall": "npx simple-git-hooks",
    "preinstall": "npx only-allow pnpm",
    "clear": "rm -rf dist"
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.1",
    "@better-scroll/observe-dom": "^2.5.1",
    "@better-scroll/pull-down": "^2.5.1",
    "@better-scroll/pull-up": "^2.5.1",
    "@iconify/iconify": "^3.1.1",
    "@unocss/reset": "^0.56.4",
    "@vueuse/core": "^10.4.1",
    "@vueuse/head": "^2.0.0",
    "crypto-js": "^4.1.1",
    "element-plus": "^2.3.14",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-i18n": "^9.4.1",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.13",
    "@iconify/json": "^2.2.125",
    "@types/crypto-js": "^4.1.2",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.7.1",
    "@types/nprogress": "^0.2.1",
    "@unocss/eslint-config": "^0.56.4",
    "@vitejs/plugin-vue": "^4.3.4",
    "eslint": "^8.50.0",
    "less": "^4.2.0",
    "lint-staged": "^14.0.1",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.2.2",
    "unocss": "^0.56.4",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^0.7.40",
    "vite-plugin-mock": "2.9.8",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vue-tsc": "^1.8.15"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
